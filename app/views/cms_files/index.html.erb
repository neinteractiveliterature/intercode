<p id="notice"><%= notice %></p>

<h1>Cms Files</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cms_files.each do |cms_file| %>
      <tr>
        <td><%= link_to 'Show', cms_file %></td>
        <td><%= link_to 'Edit', edit_cms_file_path(cms_file) %></td>
        <td><%= link_to 'Destroy', cms_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cms File', new_cms_file_path %>
