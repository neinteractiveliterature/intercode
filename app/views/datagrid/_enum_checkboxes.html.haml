= form.form_group filter.name, label: { } do
  %ul.list-inline
    - elements.each do |value, text, checked|
      - id = [form.object_name, filter.name, value].join('_').underscore
      %li.list-inline-item
        = form.check_box(filter.name, form.datagrid_extra_checkbox_options.merge(:multiple => true, :id => id, :checked => checked, :label => text), value.to_s, nil)
