= form.form_group filter.name, label: { } do
  - elements.each do |value, text, checked|
    - id = [form.object_name, filter.name, value].join('_').underscore
    = form.check_box(filter.name, form.datagrid_extra_checkbox_options.merge(:multiple => true, :id => id, :checked => checked, :label => text, :inline => true), value.to_s, nil)
