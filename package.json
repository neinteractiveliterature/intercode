  "scripts": {
    "analyze": "NODE_ENV=production ANALYZE_BUNDLE_SIZE=true webpack --progress --config=config/webpack/production.js && rm -rf public/packs",
    "analyze:json": "NODE_ENV=production webpack --profile --json --config=config/webpack/production.js >stats.json",
    "graphdoc:generate": "graphdoc -s schema.graphql",
    "lint": "eslint app/javascript --ext .js,.jsx",
    "test": "jest --testPathPattern=test/javascript --coverage",
    "test:watch": "jest --watch --testPathPattern=test/javascript"
  },
  "graphdoc": {
    "logo": "<h1 style=\"font-family: Helvetica, Arial; letter-spacing: -4px; font-size: 60px; font-weight: bold;\">Intercode</h1>"
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "app/javascript/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "./test/javascript/setupTests.js"
  },
  "browserslist": [
    "> 5%",
    "Last 2 versions",
    "IE >= 11",
    "Safari >= 9"
  ]
