  "scripts": {
    "analyze": "vite-bundle-visualizer",
    "build": "yarn run build:web && yarn run build:cli",
    "build:web": "vite build",
    "build:cli": "rollup -c rollup.cliUtils.config.ts --configPlugin swc && chmod +x ./bin/*.cjs",
    "build:timezone_data": "node ./buildTimezoneData.js",
    "css:typegen": "yarn run typed-scss-modules 'app/javascript/**/*.module.scss'",
    "graphql:build_type_data": "node ./buildGraphQLTypeData.js",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "graphql:update": "yarn run graphql:codegen && yarn run graphql:build_type_data",
    "i18next:dedupe": "yarn run tsx ./script/deduplicateTranslations.ts",
    "lint": "eslint app/javascript test/javascript schema.graphql --cache",
    "prepare": "husky install",
    "preview": "vite preview --outDir ./public",
    "start": "vite",
    "ts:check": "tsc --noEmit --incremental",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch"
  },
